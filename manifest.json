{
  "manifest_version": 2,
  "name": "Laugh Track",
  "description": "Create a chat room on any web page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "index.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' https://code.jquery.com https://cdn.socket.io; object-src 'self'"
}
